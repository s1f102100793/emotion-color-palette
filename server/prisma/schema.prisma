datasource db {
  provider = "postgresql"
  url      = env("API_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Task {
  id        String   @id
  label     String
  done      Boolean
  createdAt DateTime
}

model Color {
  id          Int      @id @default(autoincrement())
  createdAt   DateTime @default(now())
  txet        String
  paletteSize Int
  color       Int[]
  like        Int
}
