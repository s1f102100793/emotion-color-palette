datasource db {
  provider = "postgresql"
  url      = env("API_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Task {
  id        String   @id
  label     String
  done      Boolean
  createdAt DateTime
}

model Color {
  id          String   @id @default(uuid())
  createdAt   DateTime @default(now())
  name        String
  PaletteSize Int
  color       String
  iine        Int
}
